{
  "name": "smart-parking-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "vite --force",
    "server": "tsx watch server/index.ts",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:server": "eslint server/**/*.ts --fix",
    "lint:client": "eslint \"client/**/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:server": "prettier --write \"server/**/*.ts\"",
    "format:client": "prettier --write \"client/**/*.{ts,tsx}\" \"src/**/*.{ts,tsx}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:all": "node tests/run-all-tests.js",
    "test:api": "node tests/run-all-tests.js --api",
    "test:frontend": "node tests/run-all-tests.js --frontend", 
    "test:integration": "node tests/run-all-tests.js --integration",
    "test:server": "jest --testPathPattern=server/tests",
    "cleanup": "node scripts/cleanup-project.js",
    "cleanup:validate": "node scripts/cleanup-project.js --validate",
    "fix:typescript": "node scripts/fix-typescript-config.js",
    "fix:demo": "node scripts/fix-video-demo-issues.js",
    "verify:tests": "node scripts/verify-test-fixes.js",
    "test:client": "vitest --testPathPattern=src/components",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "seed": "tsx server/scripts/seedDatabase.ts",
    "seed:direct": "tsx server/scripts/seedDatabaseDirect.ts"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "recharts": "^2.15.4",
    "moment": "^2.30.1",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^10.16.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^4.21.2",
    "globals": "^15.9.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "cypress": "^13.6.0",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^23.0.1"
  }
}
